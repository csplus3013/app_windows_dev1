[
  {
    "Name": "CHECK FILE TYPE",
    "ExecutablePath": "D:\\soft\\exiftool\\check_type.cmd",
    "Arguments": "$fullpath"
  },
  {
    "Name": "CHECK SIG C2PA",
    "ExecutablePath": "D:\\soft\\c2patool\\c2patool.exe",
    "Arguments": "-d $fullpath"
  },
  {
    "Name": "CHECK C2PA valid trust",
    "ExecutablePath": "D:\\soft\\c2patool\\check_trust.cmd",
    "Arguments": "$fullpath"
  },
  {
    "Name": "CHECK Data exiftool",
    "ExecutablePath": "D:\\soft\\exiftool\\exiftool.exe",
    "Arguments": "-G1 -a -s $fullpath"
  },
  {
    "Name": "CHECK Data ffmpeg",
    "ExecutablePath": "D:\\soft\\ffmpeg\\ffprobe.exe",
    "Arguments": " -loglevel error -v quiet -show_format -show_streams -show_entries format_tags:stream_tags $fullpath"
  },
  {
    "Name": "Clear all metadata exiftool",
    "ExecutablePath": "D:\\soft\\exiftool\\exiftool.exe",
    "Arguments": "-all= -overwrite_original $fullpath"
  },
  {
    "Name": "Clear Flux data exiftool",
    "ExecutablePath": "D:\\soft\\exiftool\\exiftool.exe",
    "Arguments": "-Workflow= -Prompt= -overwrite_original $fullpath"
  },
  {
    "Name": "CLEAR all data ffmpeg",
    "ExecutablePath": "D:\\soft\\ffmpeg\\custom\\clean_video.bat",
    "Arguments": "$fullpath"
  },
  {
    "Name": "APPLY C2PA",
    "ExecutablePath": "D:\\soft\\c2patool\\c2patool.exe",
    "Arguments": "$fullpath -m d:\\soft\\c2patool\\my_manifest_image.json --no_signing_verify -o $fullpath -f"
  },
  {
    "Name": "APPLY Google ffmpeg",
    "ExecutablePath": "D:\\soft\\ffmpeg\\custom\\apply_metadata_video.bat",
    "Arguments": "$fullpath google"
  },
  {
    "Name": "APPLY Flux ffmpeg",
    "ExecutablePath": "D:\\soft\\ffmpeg\\custom\\apply_metadata_video.bat",
    "Arguments": "$fullpath flux"
  },
  {
    "Name": "APPLY LOGO on vid",
    "ExecutablePath": "D:\\soft\\ffmpeg\\custom\\apply_logo_video.bat",
    "Arguments": "$fullpath"
  }
]